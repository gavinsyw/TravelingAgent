package Serverend;

import java.util.*;
import SupportClass.*;

public class SightInfo {
	int sightNum;
	String cityName;
	
	public SightInfo(String cityName, int sightNum) {
		this.sightNum = sightNum;
	}
	
	public void writeToFile(Vector<Sight> sightVec) {
		for (int i = 0; i < sightNum; i++) {
			Sight s = sightVec.get(i);
		}
	}
}
